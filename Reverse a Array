#include <iostream>

using namespace std;

void reverse(int a[],int n){
    
    int start=0;
    int end= n-1;
    
    while(start<=end){
        swap(a[start],a[end]);
        start++;
        end--;
    }
}
int main()
{
    int a[6]={3,2,1,5,6,7};
    reverse(a,6);
    
    for(int i = 0; i <6;i++){
        cout<<a[i]<<" ";
    }

    return 0;
}
